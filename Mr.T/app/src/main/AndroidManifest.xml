        <activity
            android:name=".trafficmonitor.TrafficActivity"
            android:label="@string/title_activity_traffic" >
        </activity>
        <activity
            android:name=".DiskActivity"
            android:label="@string/title_activity_disk" >
        </activity>
        <activity
            android:name=".NoSpamActivity"
            android:label="@string/title_activity_no_spam" >
        </activity>
        <activity
            android:name=".NetControlActivity"
            android:label="@string/title_activity_net_control" >
